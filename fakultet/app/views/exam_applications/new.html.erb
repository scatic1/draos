<h1>List of exam applications</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Time</th>
      <th scope="col">Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exam_applications.each do |exam_application| %>

      <tr>
        <td><%= exam_application.exam.subject.title%></td>
        <td><%= exam_application.exam.exam_time %></td>
        <td><%= exam_application.exam.exam_place %></td>


      </tr>
<% end %>
<tr>
 <td> <%= link_to 'Back', exam_applications_path %> </td>
</tr>
  </tbody>
</table>
